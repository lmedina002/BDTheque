<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">

  <class name="Album" table="album">
    <id name="Id" column="albumId" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Cover" column="albumCover" not-null="true"/>
    <property name="Nom" column="albumNom" not-null="true"/>
    <property name="Serie" column="albumSerie" not-null="true"/>
    <property name="Auteur" column="albumAuteur" not-null="true"/>
    <property name="Categorie" column="albumCategorie" not-null="true"/>
    <property name="Genre" column="albumGenre" not-null="true"/>
    <property name="Editeur" column="albumEditeur" not-null="true"/>
  </class>

</hibernate-mapping>